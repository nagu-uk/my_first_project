<h1>Welcome</h1>
<p>To Rails Hub</p>

<table border="1px">
  <tr>
    <th> First Name </th>
    <th> Last Name </th>
    <th> DOB </th>
    <th> Age </th>
    <th> Created At </th>
   </tr>

<% User.all.each do |user| %>
    <tr>
      <td> <%= user.first_name %> </td>
      <td> <%= user.last_name %> </td>
      <td> <%= user.dob %> </td>
      <td> <%= user.age %> </td>
      <td> <%= user.created_at %> </td>
    </tr>

<% end %>
</table>